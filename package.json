  "scripts": {
    "dev": "vite",
    "dev:worker": "wrangler dev --port 8789",
    "build": "vite build",
    "preview": "vite preview",
    "db:generate": "prisma generate --schema=schema.d1.prisma",
    "db:migrate": "wrangler d1 migrations apply taxmaster-db --local",
    "db:migrate:prod": "wrangler d1 migrations apply taxmaster-db --remote",
    "deploy:worker": "wrangler deploy",
    "deploy:pages": "vite build && wrangler pages deploy dist --project-name taxmaster-frontend",
    "build:server": "esbuild src/index.ts --bundle --platform=node --target=node20 --format=cjs --outfile=dist-server/index.cjs --external:better-sqlite3 --external:mysql2 --external:pg --external:@prisma/client",
    "start": "node dist-server/index.cjs"
  },
  "dependencies": {
