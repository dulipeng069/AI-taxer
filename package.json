    "html2canvas": "^1.4.1",
    "lucide-react": "^0.555.0",
    "mysql2": "^3.15.3",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "recharts": "^3.5.1",
    "xlsx": "latest"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20251128.0",
    "@prisma/adapter-d1": "^5.10.0",
    "@prisma/client": "^5.10.0",
